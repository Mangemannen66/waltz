{
    "*://*.capitaliq.com/*": {
        "name": "CapitalIQ",
        "logout": {
            "cookies": ["sid"]
        },
        "login": {
            "urls": ["https://www.capitaliq.com/ciqdotnet/login.aspx"],
            "formURL": "https://www.capitaliq.com/ciqdotnet/login.aspx",
            "method": "POST",
            "usernameField": "myLogin$myUsername",
            "passwordField": "myLogin$myPassword",
            "hasHiddenInputs": true,
            "check": "#userNavLabel"
        }
    }
}