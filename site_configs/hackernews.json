{
    "*://news.ycombinator.com/*": {
        "name": "Hacker News",
        "logout": {
            "cookies": ["user"]
        },
        "login": {
            "urls": ["https://news.ycombinator.com/newslogin"],
            "formURL": "https://news.ycombinator.com/y",
            "method": "POST",
            "usernameField": "u",
            "passwordField": "p",
            "submitButton": "input[value='login']",
            "hasHiddenInputs": true,
            "check": "a:contains('logout')",
            "exclude": {
                "forcedRedirectURLs": ["https://news.ycombinator.com/x"],
                "nextURLs": ["https://news.ycombinator.com/x"]
            }
        }
    }
}
