{
    "*://*.google.com/*": {
        "logout": {
            "cookies": {
                "domain": "google.com",
                "names": ["SID", "GX"]
            }
        },
        "login": {
            "url": "https://accounts.google.com/ServiceLoginAuth",
            "method": "POST",
            "usernameField": "Email",
            "passwordField": "Passwd",
            "redirectField": "continue",
            "otherFields": ["GALX"]
        }
    }
}