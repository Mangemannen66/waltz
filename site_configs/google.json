{
    "*://*.google.com/*": {
        "name": "Google",
        "logout": {
            "cookies": ["SID", "GX"]
        },
        "login": {
            "url": "https://accounts.google.com/ServiceLoginAuth",
            "formURL": "https://accounts.google.com/ServiceLoginAuth",
            "method": "POST",
            "usernameField": "Email",
            "passwordField": "Passwd",
            "redirectField": "continue",
            "other": {
                "url": "https://accounts.google.com/ServiceLoginAuth",
                "fields": ["GALX"]
            },
            "check": "a#gb_71",
            "twoFactor": [
                {
                    "url": "https://accounts.google.com/SecondFactor",
                    "check": "form[action='SecondFactor']"
                }
            ]
        }
    },
    "*://*.youtube.com/*": {
        "name": "YouTube",
        "logout": {
            "cookies": ["SID", "GX"]
        },
        "login": {
            "url": "https://accounts.google.com/ServiceLoginAuth",
            "formURL": "https://accounts.google.com/ServiceLoginAuth",
            "method": "POST",
            "usernameField": "Email",
            "passwordField": "Passwd",
            "redirectField": "continue",
            "other": {
                "url": "https://accounts.google.com/ServiceLoginAuth",
                "fields": ["GALX"]
            },
            "check": "div#yt-masthead-user",
            "twoFactor": [
                {
                    "url": "https://accounts.google.com/SecondFactor",
                    "check": "form[action='SecondFactor']"
                }
            ]
        }
    }
}
