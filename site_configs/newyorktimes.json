{
    "*://*.nytimes.com/*": {
        "name": "New York Times",
        "match": "https://myaccount\\.nytimes\\.com",
        "logout": {
            "cookies": ["NYT-S"]
        },
        "login": {
            "urls": [
                "https://myaccount.nytimes.com/auth/login"
            ],
            "formURL": "https://myaccount.nytimes.com/auth/login?URI=http://",
            "method": "POST",
            "usernameField": "userid",
            "passwordField": "password",
            "hasHiddenInputs": true,
            "check": "a:contains('Log Out'), li.profileDropDownLogOut, div.dropDownModalLinkContainer.box"
        }
    }
}