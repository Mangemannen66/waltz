{
    "*://*.facebook.com/*": {
        "logout": {
            "cookies": {
                    "domain": "facebook.com",
                    "names": ["c_user"]
                }
        },
        "login": {
            "url": "https://www.facebook.com/login.php?login_attempt=1",
            "method": "POST",
            "usernameField": "email",
            "passwordField": "pass",
            "token": {
                "url": "http://facebook.com/login",
                "method": "GET",
                "selector": "some xpath or css selector"
            }
        }
    }
}