{
    "*://*.facebook.com/*": {
        "logout": {
            "cookies": {
                    "domain": "facebook.com",
                    "names": ["c_user"]
                }
        },
        "login": {
            "url": "https://www.facebook.com/login.php?login_attempt=1",
            "method": "POST",
            "usernameField": "email",
            "passwordField": "pass",
            "otherFields": [],
            "check": "input[type='submit'][value='Log Out']"
        }
    }
}