{
    "*://itunesconnect.apple.com/*": {
        "logout": {
            "cookies": ["user"]
        },
        "login": {
            "urls": ["https://itunesconnect.apple.com/WebObjects/iTunesConnect.woa/wo/14.1.9.3.13.2.1.1.3.1.1"],
            "method": "POST",
            "usernameField": "theAccountName",
            "passwordField": "theAccountPW",
            "hasHiddenInputs": true,
            "check": "a:contains('Sign Out')"
        },
        "ignore": true
    }
}

