{
    "*://*.trello.com/*": {
        "name": "Trello",
        "logout": {
            "cookies": ["token"]
        },
        "login": {
            "urls": ["https://trello.com/login"],
            "formURL": "https://trello.com/authenticate",
            "method": "POST",
            "usernameField": "user",
            "passwordField": "password",
            "hasHiddenInputs": true,
            "check": "a.header-auth.member, a.header-member"
        }
    }
}
