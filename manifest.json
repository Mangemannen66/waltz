{
  "name": "Vault",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Login Anywhere with Clef",
  "background": {
    "scripts": ["/js/delegate.js", "/js/crypto.js", "/js/storage.js"]
  },
  "content_scripts": [{
    "matches": ["https://www.facebook.com/*"],
    "js": ["js/jquery.min.js", "/js/clef.js"],
    "css": ["/css/clef.css"]
  }],
  "permissions": [
  	"https://www.facebook.com/*"
  ],
  "icons": {
    "16": "/img/clef16.png",
    "48": "/img/clef48.png",
    "128": "/img/clef128.png"
  },
  "options_page": "html/options.html",
  "homepage_url": "http://getclef.com",
  "web_accessible_resources": [
    "img/clef128.png",
    "img/clef-full.png"
  ]
}
